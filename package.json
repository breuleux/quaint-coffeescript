  "version": "0.1.0",
  "format": "cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/breuleux/quaint-coffeescript"
  },
  "keywords": [
    "quaint",
    "quaint-plugin",
    "quaint-language",
    "coffeescript"
